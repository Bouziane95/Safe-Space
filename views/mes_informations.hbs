<h1>Mes informations</h1>

    {{> informations_association}}

  <table>
    <thead>
      <tr>
        <th>Date</th>
        <th>Lieu</th>
        <th>Détails</th>
        <th>Delete</th>
      </tr>
    </thead>

    <tbody>
      {{#each infos.events}}
      <tr>
        {{> historique_mapEvents_row}}
      </tr>
      {{/each}}

      {{#unless infos.events}}
      <tr>
        <td colspan="6">Pas d'événements enregistrés</td>
      </tr>
      {{/unless}}


  
    </tbody>
  </table>
