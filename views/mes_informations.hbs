<h1>Mes informations</h1>

{{#each assos}}
  <tr>
    {{> informations_association}}
  </tr>
{{/each}}



  <table>
    <thead>
      <tr>
        <th>Date</th>
        <th>Lieu</th>
        <th>Détails</th>
        <th>Delete</th>
      </tr>
    </thead>

    <tbody>
      {{#each mapEvents}}
      <tr>
        {{> historique_mapEvents_row}}
      </tr>
      {{/each}}




      {{#unless mapEvents}}
      <tr>
        <td colspan="6">Pas d'événements enregistrés</td>
      </tr>
      {{/unless}}


  
    </tbody>
  </table>
